# Development environment for testing Swagger UI
NODE_ENV=development
PORT=3000
HOST=localhost

# Placeholder values for required environment variables
SUPABASE_URL=https://<project-ref>.supabase.co
SUPABASE_KEY=placeholder_anon_key
SUPABASE_SERVICE_KEY=REDACTED_SUPABASE_SERVICE_ROLE_KEY
JWT_SECRET=REDACTED_JWT_SECRET
JWT_EXPIRES_IN=24h

# Optional for Swagger UI testing
OPENAI_API_KEY=REDACTED_OPENAI_API_KEY
LOG_LEVEL=info
ENABLE_METRICS=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
API_VERSION=v1
API_PREFIX=/api
# Development environment for MaaS service
NODE_ENV=development
PORT=3000
HOST=localhost

# Central Auth Gateway Configuration
CORE_API_BASE_URL=https://api.lanonasis.com
AUTH_GATEWAY_URL=https://api.lanonasis.com
SERVICE_ID=lanonasis-maas

# Legacy Supabase Configuration (server-only, will be phased out)
SUPABASE_URL=https://<project-ref>.supabase.co
SUPABASE_SERVICE_KEY=REDACTED_SUPABASE_SERVICE_ROLE_KEY

# Service Configuration
JWT_SECRET=REDACTED_JWT_SECRET
JWT_EXPIRES_IN=24h
OPENAI_API_KEY=REDACTED_OPENAI_API_KEY

# API Configuration
LOG_LEVEL=info
ENABLE_METRICS=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
API_VERSION=v1
API_PREFIX=/api
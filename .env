# Development environment for testing Swagger UI
NODE_ENV=development
PORT=3000
HOST=localhost

# Placeholder values for required environment variables
SUPABASE_URL=https://placeholder.supabase.co
SUPABASE_KEY=placeholder_anon_key
SUPABASE_SERVICE_KEY=placeholder_service_key
JWT_SECRET=placeholder_jwt_secret_at_least_32_characters_long_for_development
JWT_EXPIRES_IN=24h

# Optional for Swagger UI testing
OPENAI_API_KEY=placeholder_openai_key
LOG_LEVEL=info
ENABLE_METRICS=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
API_VERSION=v1
API_PREFIX=/api
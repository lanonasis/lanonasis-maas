# Development environment for MaaS service
NODE_ENV=development
PORT=3000
HOST=localhost

# Central Auth Gateway Configuration
CORE_API_BASE_URL=https://api.lanonasis.com
AUTH_GATEWAY_URL=https://api.lanonasis.com
SERVICE_ID=lanonasis-maas

# Legacy Supabase Configuration (server-only, will be phased out)
SUPABASE_URL=https://placeholder.supabase.co
SUPABASE_SERVICE_KEY=placeholder_service_key

# Service Configuration
JWT_SECRET=placeholder_jwt_secret_at_least_32_characters_long_for_development
JWT_EXPIRES_IN=24h
OPENAI_API_KEY=placeholder_openai_key

# API Configuration
LOG_LEVEL=info
ENABLE_METRICS=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
API_VERSION=v1
API_PREFIX=/api
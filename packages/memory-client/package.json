{
  "name": "@LanOnasis/memory-client",
  "version": "1.3.0",
  "description": "Memory as a Service (MaaS) Client SDK for LanOnasis - Intelligent memory management with CLI/MCP integration and semantic search",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "memory",
    "ai",
    "semantic-search",
    "knowledge-management",
    "LanOnasis",
    "vector-search",
    "sdk",
    "api-client",
    "typescript",
    "memory-as-a-service",
    "cli-integration",
    "mcp",
    "model-context-protocol"
  ],
  "author": {
    "name": "LanOnasis Team",
    "email": "team@LanOnasis.com",
    "url": "https://LanOnasis.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LanOnasis/memory-client.git"
  },
  "homepage": "https://docs.LanOnasis.com/sdk",
  "bugs": {
    "url": "https://github.com/LanOnasis/memory-client/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "optionalDependencies": {
    "child_process": "*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.28.1",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/LanOnasis"
  }
}
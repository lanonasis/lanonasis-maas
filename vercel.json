{
  "version": 2,
  "name": "lanonasis-developer",
  "builds": [
    {
      "src": "dashboard/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/functions/api.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/functions/mcp-sse.js", 
      "use": "@vercel/node"
    },
    {
      "src": "api/functions/orchestrate.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/functions/api.js": {
      "maxDuration": 30
    },
    "api/functions/mcp-sse.js": {
      "maxDuration": 300
    },
    "api/functions/orchestrate.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/v1/health",
      "destination": "/api/functions/api"
    },
    {
      "source": "/api/v1/orchestrate",
      "destination": "/api/functions/orchestrate"
    },
    {
      "source": "/api/v1/(.*)",
      "destination": "/api/functions/api"
    },
    {
      "source": "/mcp/sse",
      "destination": "/api/functions/mcp-sse"
    },
    {
      "source": "/sse",
      "destination": "/api/functions/mcp-sse"
    },
    {
      "source": "/health",
      "destination": "/api/functions/api"
    },
    {
      "source": "/metrics",
      "destination": "/api/functions/api"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/dashboard/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/docs",
      "destination": "/docs/index.html"
    },
    {
      "source": "/docs/(.*)",
      "destination": "/docs/$1"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, X-API-Key"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/mcp/sse",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/event-stream"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache"
        },
        {
          "key": "Connection",
          "value": "keep-alive"
        }
      ]
    }
  ],
  "env": {
    "NODE_VERSION": "18",
    "NODE_ENV": "production"
  },
  "regions": ["iad1"]
}